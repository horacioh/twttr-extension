{
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "Twitter Zend",
  "description": "",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "host_permissions": ["https://twitter.com"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["main.css"]
    }
  ]
}
